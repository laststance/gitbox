{
  "name": "gitbox",
  "version": "0.1.0",
  "private": true,
  "description": "A web application for managing GitHub repositories in Kanban board format",
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6",
  "volta": {
    "node": "24.12.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,ts,jsx,tsx,cjs,mjs}": [
      "eslint -c eslint.config.mjs --fix --concurrency=auto --max-warnings 0 --no-warn-ignored"
    ]
  },
  "scripts": {
    "dev": "next dev -p 3008",
    "dev:webpack": "next dev -p 3008 --webpack",
    "build": "next build",
    "start": "next start -p 3008",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --concurrency=auto --max-warnings 0",
    "lint:fix": "eslint . --fix --concurrency=auto --max-warnings 0",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky",
    "prettier": "prettier --ignore-unknown --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gitbox/redux-storage-middleware": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
